{
  "hosting": {
    "public": ".next",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "*",
        "has": [
          {
            "type": "header",
            "key": "Origin",
            "value": "(?<origin>.*)"
          }
        ],
        "destination": "/.next/server/edge-chunks/prerender-manifest.js"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]

  },"emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  } 
}
